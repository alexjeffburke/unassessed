    "mkdirp": "^0.5.1",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "prettier": "~1.19.1",
    "rollup": "^1.27.8",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.1.0",
